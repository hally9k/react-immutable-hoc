{
    "name": "react-immutable-hoc",
    "version": "1.0.0",
    "description":
        "Higher order component for turning props that are ImmutableJS objects into plain JS.",
    "main": "dist/index.js",
    "scripts": {
        "build": "babel src --out-dir dist --ignore **/*.test.js",
        "test": "jest --config=\"./jest.config.json\"",
        "test-debug":
            "node --inspect ./node_modules/.bin/jest --watch --runInBand --config=\"./jest.config.json\"",
        "lint": "eslint src",
        "type-check": "flow",
        "format": "prettier-eslint \"src/**/*.{js,jsx}\" --write",
        "precommit": "npm run lint && npm run type-check",
        "prepush": "npm t"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hally9k/react-immutable-hoc.git"
    },
    "keywords": ["react", "hoc", "immutable", "redux"],
    "author": "hally9k",
    "contributors": [
        {
            "name": "David Seaman",
            "url": "https://github.com/ctrldavid"
        },
        {
            "name": "Jacob Foley",
            "url": "https://github.com/jacobjfoley"
        }
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/hally9k/react-immutable-hoc/issues"
    },
    "homepage": "https://github.com/hally9k/react-immutable-hoc#readme",
    "dependencies": {
        "immutable": "^4.0.0-rc.9",
        "react": "^16.0.0"
    },
    "peerDependencies": {
        "immutable": ">=4.x.x",
        "react": ">=16.x.x"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^8.0.1",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-react": "^6.24.1",
        "enzyme": "^3.1.0",
        "enzyme-adapter-react-16": "^1.0.2",
        "eslint": "^4.9.0",
        "eslint-plugin-babel": "^4.1.2",
        "eslint-plugin-compat": "^2.0.1",
        "eslint-plugin-filenames": "^1.2.0",
        "eslint-plugin-flowtype": "^2.35.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jest": "^21.2.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.1.0",
        "flow-bin": "^0.57.3",
        "flow-typed": "^2.2.0",
        "fs": "0.0.2",
        "husky": "^4.0.6",
        "jest": "^21.2.1",
        "path": "^0.12.7",
        "prettier-eslint-cli": "^4.4.0",
        "react-dom": "^16.0.0",
        "release": "^2.4.0",
        "webpack": "^3.6.0"
    }
}
